{
  "presets": [ "es2015", "react", "stage-0"],
  "plugins": [["transform-class-properties"]],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }, {
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          },{
            "transform": "react-transform-render-visualizer"
          }]
        }]
      ]
    }
  }
}
